@source(type = 'kafka', topic.list = 'RawTopic', partition.no.list = '0', threading.option = 'single.thread', group.id = 'group', bootstrap.servers = 'localhost:9092',@map(type = 'json', enclosing.element = '$', @attributes(object = 'identity.objectID', tenant = 'identity.tenantID', domain= 'source.domain')))   define stream SweetProductionStream (object string, tenant string, domain string);   @sink(type = 'kafka', topic = 'EventTopicQueue', bootstrap.servers = 'localhost:9092', partition.no = '0',@map(type = 'json'))   define stream LowProductionAlertStream (object string, tenant string, domain string);   @info(name = 'query1') from SweetProductionStream   select *   insert into LowProductionAlertStream;